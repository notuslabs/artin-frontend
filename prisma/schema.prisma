generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model UserImage {
  id        Int    @id @default(autoincrement())
  image_url String @db.VarChar(500)
  address   String @db.VarChar(500)
  prompt    String @db.VarChar(500)

  @@index([address])
}
