generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model UserImage {
  id        Int    @id @default(autoincrement())
  image_url String @db.LongText
  address   String @db.Char(42)
  prompt    String @db.LongText

  @@index([address])
}
